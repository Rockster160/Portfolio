<% list.sectioned_objects.each do |obj| %>
  <% if obj[:type] == :item %>
    <%= render template: "list_items/show", locals: { list: list, item: obj[:object] }, layout: false %>
  <% elsif obj[:type] == :section %>
    <%= render template: "sections/show", locals: { list: list, obj: obj }, layout: false %>
  <% end %>
<% end %>
