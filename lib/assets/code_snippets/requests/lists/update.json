curl "http://ardesian.com/lists/:list_id/modify_from_message" \
  -u "username:password" \
  --data-urlencode "message=add potatoes, carrots, and onions"

curl "http://ardesian.com/lists/:list_id/modify_from_message" \
  -u "username:password" \
  --data-urlencode "message=remove potatoes, carrots, and onions"

curl "http://ardesian.com/lists/:list_id/modify_from_message" \
  -u "username:password" \
  --data-urlencode "message=clear"
